<?xml version="1.0" encoding="utf-8"?>

<manifest
    xmlns:android="http://schemas.android.com/apk/res/android" package="group.asteriskint.adm">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/asterisk_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/asterisk_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".auth.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.RegisterActivity" />
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".ui.activity.HomeActivity" />
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>
